# ==============================================================================
# Nomad AI Agent - Production Environment Configuration
#
# Copy this file to .env in production and fill in actual values
# NEVER commit .env file with real credentials to version control!
# ==============================================================================

# --- API Keys (REQUIRED) ---
GEMINI_API_KEY="AIzaSyCg80yYhLrN0RRGXF4BcAUbCWOvX77LHas"
OPENROUTER_API_KEY="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"

# --- Server Configuration ---
NOMAD_PORT=8080
NOMAD_HOST=0.0.0.0
NOMAD_ENV=production

# --- Logging ---
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=/opt/nomad/logs/nomad.log
LOG_MAX_BYTES=10485760
LOG_BACKUP_COUNT=5

# --- Resource Limits ---
MAX_CONCURRENT_MISSIONS=5
MAX_TOKENS_PER_REQUEST=100000
BUDGET_LIMIT_USD=10.0

# --- LLM Configuration ---
DEFAULT_LLM_PROVIDER=openrouter
DEFAULT_MODEL=google/gemini-2.0-flash-exp
FALLBACK_MODEL=google/gemini-1.5-flash
TEMPERATURE=0.7
MAX_RETRIES=3

# --- Health Monitor ---
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=10
CPU_THRESHOLD=80.0
MEMORY_THRESHOLD=85.0

# --- Security ---
CORS_ORIGINS=https://your-domain.com
ALLOWED_HOSTS=your-domain.com,*.your-domain.com
API_KEY_ROTATION_DAYS=90

# --- Database (if used) ---
# DATABASE_URL=postgresql://user:pass@localhost/nomad

# --- Monitoring (optional) ---
# SENTRY_DSN=https://your-sentry-dsn
# PROMETHEUS_PORT=9090

# --- Backup ---
BACKUP_ENABLED=true
BACKUP_DIRECTORY=/opt/nomad/backups
BACKUP_RETENTION_DAYS=30

# --- Rate Limiting ---
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60
